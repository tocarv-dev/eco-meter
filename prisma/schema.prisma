generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL")
}

model Surveys {
  id String @id @default(uuid())
  userid String
  createdAt DateTime @default(now())

  gender String
  age Int
  municipality String

  meals_vegan Int
  meals_redmeat Int
  meals_whitemeat Int

  flights_hour Int?
  flights_short Int?
  flights_medium Int?
  flights_long Int?

  bags_unsorted Int
  bags_paper Int?
  bags_plastic Int?
  bags_glass Int?
  bags_organic Int?

  home_residents Int 
  home_eletricity Int

  gas_type String?
  gas_spend Int?

  useWood Boolean @default(false)

  transport_dieselCar Int?
  transport_gasCar Int?
  transport_hybridCar Int?
  transport_gplCar Int?
  transport_eletricCar Int?
  transport_motorcycle Int?
  transport_taxi Int?
  transport_train Int?
  transport_bus Int?
  transport_subway Int?
  transport_tram Int?
  transport_ferryFoot Int?
  transport_ferryCar Int?
  
  profile String
}